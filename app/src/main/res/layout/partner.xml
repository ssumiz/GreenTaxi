<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".notice"
    android:background="#FFFF"
    android:focusable="true"
    android:focusableInTouchMode="true">

    <com.ssomai.android.scalablelayout.ScalableLayout
        android:id="@+id/sl1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:scale_base_height="1600"
        app:scale_base_width="1080">

        <TextView
            android:id="@+id/signUp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_margin="25dp"
            android:layout_weight="1"
            android:gravity="center"
            android:text="[ 파트너 목록 ]"
            android:textColor="#000"
            android:textSize="40dp"
            android:textStyle="normal"
            app:scale_height="250"
            app:scale_textsize="100"
            app:scale_width="1080"
            tools:ignore="MissingPrefix" />

        <ImageButton
            android:id="@+id/partner_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            app:scale_height="130"
            app:scale_left="50"
            app:scale_top="60"
            app:scale_width="130"
            app:srcCompat="@drawable/back"
            tools:ignore="MissingPrefix" />

        <TextView

            android:layout_width="359dp"
            android:layout_height="match_parent"
            android:background="@drawable/greenline"
            android:ems="2"
            android:gravity=""
            app:scale_height="150"
            app:scale_left="105"
            app:scale_top="230"
            app:scale_width="880"
            tools:ignore="MissingPrefix" />
        //첫번째줄

        <TextView

            android:layout_width="359dp"
            android:layout_height="match_parent"
            android:background="@drawable/greenline"
            android:ems="2"
            android:gravity=""
            app:scale_height="150"
            app:scale_left="105"
            app:scale_top="380"
            app:scale_width="880"
            tools:ignore="MissingPrefix" />
        //두번째줄

        <TextView

            android:layout_width="359dp"
            android:layout_height="match_parent"
            android:background="@drawable/greenline"
            android:ems="2"
            android:gravity=""
            app:scale_height="150"
            app:scale_left="105"
            app:scale_top="530"
            app:scale_width="880"
            tools:ignore="MissingPrefix" />
        //세번째줄
        <TextView

            android:layout_width="359dp"
            android:layout_height="match_parent"
            android:background="@drawable/greenline"
            android:ems="2"
            android:gravity=""
            app:scale_height="150"
            app:scale_left="105"
            app:scale_top="680"
            app:scale_width="880"
            tools:ignore="MissingPrefix" />
        //네번째줄

        <TextView

            android:layout_width="359dp"
            android:layout_height="match_parent"
            android:background="@drawable/greenline"
            android:ems="2"
            android:gravity=""
            app:scale_height="150"
            app:scale_left="105"
            app:scale_top="830"
            app:scale_width="880"
            tools:ignore="MissingPrefix" />
        //다섯번째줄

        <TextView

            android:layout_width="359dp"
            android:layout_height="match_parent"
            android:background="@drawable/greenline"
            android:ems="2"
            android:gravity=""
            app:scale_height="150"
            app:scale_left="105"
            app:scale_top="980"
            app:scale_width="880"
            tools:ignore="MissingPrefix" />
        //여섯번째줄

        <TextView

            android:layout_width="359dp"
            android:layout_height="match_parent"
            android:background="@drawable/greenline"
            android:ems="2"
            android:gravity=""
            app:scale_height="150"
            app:scale_left="105"
            app:scale_top="1130"
            app:scale_width="880"
            tools:ignore="MissingPrefix" />
        //일곱째줄

        <TextView

            android:layout_width="359dp"
            android:layout_height="match_parent"
            android:background="@drawable/greenline"
            android:ems="2"
            android:gravity=""
            app:scale_height="150"
            app:scale_left="105"
            app:scale_top="1280"
            app:scale_width="880"
            tools:ignore="MissingPrefix" />
        //여덟째줄


        <ImageView
            android:onClick="onClick"
            android:id="@+id/speaker"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:scale_height="80"
            app:scale_left="650"
            app:scale_top="340"
            app:scale_width="170"
            app:srcCompat="@drawable/speaker1"

            tools:ignore="MissingPrefix" />
        <ImageView
            android:onClick="onClick"
            android:id="@+id/pluse"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:scale_height="80"
            app:scale_left="740"
            app:scale_top="340"
            app:scale_width="170"
            app:srcCompat="@drawable/plus"

            tools:ignore="MissingPrefix" />

        <ImageView
            android:onClick="onClick"
            android:id="@+id/Munus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:scale_height="80"
            app:scale_left="840"
            app:scale_top="340"
            app:scale_width="170"
            app:srcCompat="@drawable/minus"

            tools:ignore="MissingPrefix" />


        <ImageView
            android:onClick="onClick"
            android:id="@+id/speaker1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:scale_height="80"
            app:scale_left="650"
            app:scale_top="490"
            app:scale_width="170"
            app:srcCompat="@drawable/speaker2"

            tools:ignore="MissingPrefix" />
        <ImageView
            android:onClick="onClick"
            android:id="@+id/pluse1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:scale_height="80"
            app:scale_left="740"
            app:scale_top="490"
            app:scale_width="170"
            app:srcCompat="@drawable/plus"

            tools:ignore="MissingPrefix" />

        <ImageView
            android:onClick="onClick"
            android:id="@+id/Munus1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:scale_height="80"
            app:scale_left="840"
            app:scale_top="490"
            app:scale_width="170"
            app:srcCompat="@drawable/minus"

            tools:ignore="MissingPrefix" />

        <Button
            android:id="@+id/partner_ok"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="신청목록"
            app:scale_height="130"
            app:scale_width="200"
            app:scale_left="550"
            app:scale_textsize="40"
            app:scale_top="1400"
            tools:ignore="MissingPrefix"/>

        <Button
            android:id="@+id/partner_cancel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:onClick="onClick"
            android:text="편집"
            app:scale_height="130"
            app:scale_width="180"
            app:scale_left="780"
            app:scale_textsize="40"
            app:scale_top="1400"
            tools:ignore="MissingPrefix"/>

        <ListView
            android:id = "@+id/partner_list"
            app:scale_height="1300"
            app:scale_width="800"
            app:scale_left="100"
            app:scale_textsize="40"
            app:scale_top="300"
            tools:ignore="MissingPrefix"
            android:layout_width="1302dp"
            android:layout_height="183dp" />


    </com.ssomai.android.scalablelayout.ScalableLayout>
</RelativeLayout>